import wollok.game.*
import movimiento.*
import teclado.*
import enemigos.*
import heroes.*
import niveles.*


program juego {
	
	const movimiento = game.sound("./musica/movimiento.mp3")
    movimiento.shouldLoop(true)
    game.schedule(500, { movimiento.play()})
	
	game.cellSize(100)
	game.width(50)
	game.height(50)
	
    nivelIntro.inicio()
    game.start()
}


